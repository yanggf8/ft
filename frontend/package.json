{
  "name": "fortunet-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && git rev-parse HEAD > dist/.build-info",
    "preview": "vite preview",
    "test": "vitest",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=fortunet",
    "deploy:prod": "npm run build && wrangler pages deploy dist --project-name=fortunet --branch=main"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^2.0.0",
    "jsdom": "^25.0.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vitest": "^2.0.0",
    "wrangler": "^4.53.0"
  }
}
